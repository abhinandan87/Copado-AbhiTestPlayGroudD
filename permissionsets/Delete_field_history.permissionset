<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This permission set allows users to delete field history records for objects with history tracking enabled.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Delete field history</label>
    <license>Salesforce</license>
    <userPermissions>
        <enabled>true</enabled>
        <name>DeleteFieldHistory</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>DeleteFieldHistoryArchive</name>
    </userPermissions>
</PermissionSet>