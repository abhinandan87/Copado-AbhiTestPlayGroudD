    <fields>
        <fullName>TaxCountry__c</fullName>
        <description>SFDC1-215. Contains the Tax Country field pulled in from the product. Complies with the US Supreme Court Wayfair Decision and Tax Reform Act effective Jan. 1, 2019.</description>
        <externalId>false</externalId>
        <formula>PricebookEntry.Product2.TaxCountry__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Complies with the US Supreme Court Wayfair Decision and Tax Reform Act effective Jan. 1, 2019.</inlineHelpText>
        <label>Tax Country/Territory</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>TotalDiscountValue__c</fullName>
        <description>SFDC1-173 This denotes the total discount.</description>
        <externalId>false</externalId>
        <formula>(UnitListPrice__c -Discount_Amount__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Updated List Price with the Discount Amount or Discount % applied</inlineHelpText>
        <label>Discounted Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>TotalPrice</fullName>
    </fields>
    <fields>
        <fullName>UnitListPrice__c</fullName>
        <defaultValue>0.00</defaultValue>
        <description>SFDC1-173 Currency field that displays the intial value pre discount as entered by the rep</description>
        <externalId>false</externalId>
        <inlineHelpText>List Price for the product populated by sales from pricing tools or through renewal process</inlineHelpText>
        <label>Updated List Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>UnitPrice</fullName>
    </fields>
    <fields>
        <fullName>WeightedValueatRisk__c</fullName>
        <externalId>false</externalId>
        <formula>EstimatedValueatRisk__c  *  RiskProbability__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Weighted Value at Risk</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <validationRules>
        <fullName>CCIDAndCCQuoteIdSalesGeneralEdit</fullName>
        <active>true</active>
        <description>SFDC-2424 : sales needs to edit CCID and CCQUOTEID on opportunity Product.</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
OR(ISCHANGED( CCID__c ), ISCHANGED( CCQuoteID__c )),
NOT(OR(Opportunity.RecordType.Name = &quot;New Business&quot;, 
	Opportunity.RecordType.Name = &quot;Forecasting Placeholder&quot;,
Opportunity.RecordType.Name = &quot;Renewal&quot;)
),
		CONTAINS($Profile.Name, &quot;General&quot;)
)
)</errorConditionFormula>
        <errorMessage>CCID and CCQuoteId can only be edited for New Business Opportunities, Auto-Renewal Opportunities with changes or Non Auto-Renewal Opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractEndDtCantExceedContractStartDt</fullName>
        <active>true</active>
        <description>SFDC1-7027: The Contract End Date cannot precede the Contract Start Date on the Opportunity Product</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,

AND(NOT(ISNULL(ContractEndDate__c)), NOT(ISNULL(ContractStartDate__c)), ContractEndDate__c &lt;  ContractStartDate__c  )
&amp;&amp;(NOT ($Permission.ValidationByPassPermission ))
)</errorConditionFormula>
        <errorDisplayField>ContractEndDate__c</errorDisplayField>
        <errorMessage>Contract End Date needs to be greater than or equal to the Contract Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractEndDtCantPrecedeOppContractEndDt</fullName>
        <active>true</active>
        <description>SFDC1-7027: The Contract End Date can not exceed the Opportunity’s Contract End Date</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
OR(AND(NOT(ISNULL(ContractEndDate__c)), ContractEndDate__c &gt; Opportunity.ContractEndDate__c ),
AND(NOT(ISNULL(ContractEndDate__c)),ISNULL(Opportunity.ContractEndDate__c))
)
&amp;&amp;(NOT ($Permission.ValidationByPassPermission ))
)</errorConditionFormula>
        <errorDisplayField>ContractEndDate__c</errorDisplayField>
        <errorMessage>Dates need to be within Opportunity Contract Dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractStartDtCantPrecedeOppConStartDt</fullName>
        <active>true</active>
        <description>SFDC1-7027: The Contract Start Date can not precede the Opportunity’s Contract Start Date</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
OR(AND(NOT(ISNULL(ContractStartDate__c)), ContractStartDate__c &lt; Opportunity.ContractStartDate__c ),
AND(NOT(ISNULL(ContractStartDate__c)),ISNULL(Opportunity.ContractStartDate__c))
)
&amp;&amp;(NOT ($Permission.ValidationByPassPermission ))
)</errorConditionFormula>
        <errorDisplayField>ContractStartDate__c</errorDisplayField>
        <errorMessage>Dates need to be within Opportunity Contract Dates</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EstimatedRiskExceedsAnnualizedAmount</fullName>
        <active>false</active>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
        EstimatedValueatRisk__c &gt;  AnnualizedTotalAmount__c
        )</errorConditionFormula>
        <errorDisplayField>EstimatedValueatRisk__c</errorDisplayField>
        <errorMessage>Estimated Value at Risk shouldn’t be greater than Annualized Total Amount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EstimatedRiskExceedsPriorYearSalesPrice</fullName>
        <active>true</active>
        <description>Estimated Value at Risk cannot be greater than Prior Year Total Sales Price</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
OR(
 EstimatedValueatRisk__c &gt; PriorYearTotalSalesPrice__c,
 AND(ISNULL(PriorYearTotalSalesPrice__c),
     EstimatedValueatRisk__c != 0
    )
 )
)</errorConditionFormula>
        <errorDisplayField>EstimatedValueatRisk__c</errorDisplayField>
        <errorMessage>Estimated Value at Risk shouldn’t be greater than Prior Year Total Sales Price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>InnerYearRecordValidationCheck</fullName>
        <active>false</active>
        <description>SFDC1-7676 Disable modification for everyone but system admin.</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( 
   $Profile.Name &lt;&gt; &apos;IHSMarkit System Admin&apos;, 
   $Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
   Opportunity.RecordType.Name = &apos;Inner Year&apos;,
   NOT($Permission.AllowInnerYearOpptyLineItemUpdate) 
)
)</errorConditionFormula>
        <errorMessage>You are not authorized to update Opportunity Product values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityGratisProductValueZero</fullName>
        <active>true</active>
        <description>SFDC1-6554;Users cannot put an amount for gratis opportunity in opportunity product</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( (Opportunity.RecordType.Name = &apos;Gratis&apos; ),
    (TotalPrice &gt; 0))
)</errorConditionFormula>
        <errorMessage>Please note Opportunity Product Total price for Gratis Opportunity should be zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityProductStatusAccess</fullName>
        <active>false</active>
        <description>SFDC-1788
Allows editing of the opportunity product status if the user has the &quot;Renewal Opportunity Product Status&quot; or is a System Admin.</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
	ISPICKVAL(Opportunity.SourceSystem__c, &quot;SAP&quot;),
	$Permission.RenewalOpportunityProductStatus,
	ISCHANGED(Status__c),
	Opportunity.RecordTypeId = &quot;0120P000000kKaZ&quot;
	)
	)</errorConditionFormula>
        <errorMessage>You can only edit this field if the renewal opportunity is manually created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OppproductStatusCanNotChangedFromNew</fullName>
        <active>true</active>
        <description>SFDC1-5780 : Once Opportunity Product status is set to New, can not be Changed back to Existing.By passing this validation rule for admin and API  profile as per ticket SFDC1-6247
SFDC-701 allow Sales Ops to bypass this validation rule</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND ( 
    TEXT(PRIORVALUE(Status__c))  = &apos;New&apos;,
    OR (
        TEXT(Status__c) =&apos;Existing&apos;,
        TEXT(Status__c) =&apos;&apos;
    ),
    $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
    $Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;, 
    $Profile.Name &lt;&gt; &quot;IHSMarkit System Admin: API&quot;,
    NOT(CONTAINS($Profile.Name, &quot;Sales Ops&quot;)),
    NOT(CONTAINS($Profile.Name, &quot;User Support&quot;)),
    NOT($Permission.RenewalOpportunityProductStatus),
    NOT($Permission.EnhancedSalesPermission)
)
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status can not be changed from New to Existing or None. If you wish to remove it, please delete the product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProbabilityRangeError</fullName>
        <active>true</active>
        <description>SFDC-1924 : Probability must have a minimum value of 0 and maximum value of 100.</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
OR(
   Probability__c &gt; 1.0,
   Probability__c &lt; 0.0
)
)</errorConditionFormula>
        <errorMessage>Probability must have a value between 0 and 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProdCannotbeAtRiskWithNewLostStatus</fullName>
        <active>true</active>
        <description>The product can not be at Risk when the status is New/Loss. Only existing Product can be marked at risk</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
NOT(ISPICKVAL( Status__c , &apos;Existing&apos;)),
ISPICKVAL( IsProductatRisk__c , &apos;Yes&apos;),
ISPICKVAL(  RiskStatus__c , &apos;Open - Pending Resolution&apos;)
)
)</errorConditionFormula>
        <errorDisplayField>IsProductatRisk__c</errorDisplayField>
        <errorMessage>Product can not be at Risk when the status is New/Loss</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RenewalsLicenseNumberEditableOnlyToAPI</fullName>
        <active>true</active>
        <description>SFDC1-4689 License Number on Renewals Opportunity Products is editable only to &quot;IHSMarkit System Admin: API&quot; profile</description>
        <errorConditionFormula>If(OR($Setup.SystemMutedValidationRules__c.OpportunityLineItem__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
ISCHANGED(LicenseNumber__c)
&amp;&amp; ISPICKVAL(Status__c, &apos;Existing&apos;)
&amp;&amp; $Profile.Name != &apos;IHSMarkit System Admin: API&apos;
&amp;&amp; $Profile.Name != &apos;System Administrator&apos;
)</errorConditionFormula>
        <errorDisplayField>LicenseNumber__c</errorDisplayField>
        <errorMessage>License Number on Renewals Opportunity Products is editable only to &quot;IHSMarkit System Admin: API&quot; profile</errorMessage>
    </validationRules>
</CustomObject>