    <fields>
        <fullName>LeadOwner__c</fullName>
        <description>SFDC-3209 - Created to display Lead Owner Name on Related List View</description>
        <externalId>false</externalId>
        <formula>IF( LEFT(Lead.OwnerId, 3) = &apos;005&apos;, 
Lead.Owner:User.FullName__c,
Lead.Owner:Queue.QueueName 
)</formula>
        <label>Lead Owner</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
