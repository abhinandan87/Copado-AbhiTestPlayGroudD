        <label>Custom Link Name</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Domain__c</fullName>
        <description>SFDC-2564: Created a new field that determines the type of Custom Link.</description>
        <externalId>false</externalId>
        <inlineHelpText>Type of Custom Link.</inlineHelpText>
        <label>Domain</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Global</fullName>
                    <default>false</default>
                    <label>Global</label>
                </value>
                <value>
                    <fullName>Account</fullName>
                    <default>false</default>
                    <label>Account</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>InternalLink__c</fullName>
        <defaultValue>false</defaultValue>
        <description>SFDC-2564 To identify if the URL is an internal link.</description>
        <externalId>false</externalId>
        <label>Internal Link?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>RelatedField__c</fullName>
        <description>SFDC-2564 Created to add API name(s) of the field(s) that has to be used in the URL field.</description>
        <externalId>false</externalId>
        <inlineHelpText>To add API name of the field. This will replace &apos;{FieldValue0}&apos; string in the URL.
In case of multiple fields to be used in the URL, add comma separated API names, in the same sequence as {FieldValue(i)} in the URL field.</inlineHelpText>
        <label>Related Field</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SortOrder__c</fullName>
        <description>SFDC-2564: This field determines the order in which the Custom Links would appear</description>
        <externalId>false</externalId>
        <inlineHelpText>Add order number of the custom link.</inlineHelpText>
        <label>Sort Order</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>URL__c</fullName>
        <description>SFDC1-5622 - Custom Link&apos;s URL</description>
        <externalId>false</externalId>
        <inlineHelpText>Please enter the URL. 
To add a field value as a parameter, use &apos;{FieldValue0}&apos;. In case of multiple parameters, use &apos;{FieldValue0}&apos;, &apos;{FieldValue1}&apos;, &apos;{FieldValue2}&apos; and so on. Eg: www.example.com?p0={FieldValue0}&amp;p1={FieldValue1}&amp;p2={FieldValue2}.</inlineHelpText>
        <label>URL</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
