    <validationRules>
        <fullName>ContractCurrencyCheck</fullName>
        <active>true</active>
        <description>SFDC-1438 - Volume/Variable Billing Line Item Currency
SFDC-525 - Added System Admin bypass</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ContractLineItemSchedule__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
Contract__r.Currency__c &lt;&gt; TEXT(CurrencyIsoCode),
$Profile.Name != &apos;System Administrator&apos;,
$Profile.Name != &apos;IHSMarkit System Admin&apos;,
NOT(CONTAINS($Profile.Name, &quot;Contract Management&quot;)),
NOT(CONTAINS($Profile.Name, &quot;User Support&quot;)))
)</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Incorrect Currency Code, Please Refer to the Contract and Update Accordingly</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CorrectContractNumberOnCLIS</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ContractLineItemSchedule__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
ISNEW(),
OR(
Contract__c &lt;&gt; Contract__r.Id,
ContractLineItem__r.Contract__c &lt;&gt; Contract__c 
)
)
)</errorConditionFormula>
        <errorDisplayField>Contract__c</errorDisplayField>
        <errorMessage>You must select the contract related to this fixed fee line.</errorMessage>
    </validationRules>
