                CONTAINS($Profile.Name,&apos;User Support&apos;),
                CONTAINS($Profile.Name,&apos;Order Management&apos;)
            ),
            AND(
                NOT(ISPICKVAL(PRIORVALUE(StageName),&quot;6W-Closed Won&quot;)),
                NOT(ISPICKVAL(PRIORVALUE(StageName),&quot;6W-Renewal Won&quot;))
            ),
            TRUE
        ),
        FALSE
    )
)
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Only Order Management can set the Sales Stage to &quot;6WP-Closed Won/Order Processed&quot;</errorMessage>
    </validationRules>
