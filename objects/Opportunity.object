    <validationRules>
        <fullName>InnerYearRecordValidationCheck</fullName>
        <active>true</active>
        <description>SFDC1-7676:The business does not want anyone making changes to fields on an inner year opportunity, SFDC-2799 CSD &amp; CED added; also EnhancedSalesPermission</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
