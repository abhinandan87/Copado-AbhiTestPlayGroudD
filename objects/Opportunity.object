        <externalId>false</externalId>
        <formula>IF( YEAR(ContractStartDate__c) &gt; 2021,
IF(OR(MONTH(ContractStartDate__c) = 1, MONTH(ContractStartDate__c) = 2, MONTH(ContractStartDate__c) = 3), TEXT(YEAR(ContractStartDate__c))&amp;&apos;-Q1&apos;,
IF(OR(MONTH(ContractStartDate__c) = 4, MONTH(ContractStartDate__c) = 5, MONTH(ContractStartDate__c) = 6), TEXT(YEAR(ContractStartDate__c))&amp;&apos;-Q2&apos;,
IF(OR(MONTH(ContractStartDate__c) = 7, MONTH(ContractStartDate__c) = 8, MONTH(ContractStartDate__c) = 9), TEXT(YEAR(ContractStartDate__c))&amp;&apos;-Q3&apos;,
IF(OR(MONTH(ContractStartDate__c) = 10, MONTH(ContractStartDate__c) = 11, MONTH(ContractStartDate__c) = 12), TEXT(YEAR(ContractStartDate__c))&amp;&apos;-Q4&apos;,null)))),
IF( YEAR(ContractStartDate__c) &lt; 2022,IF(OR(MONTH(ContractStartDate__c) = 12, MONTH(ContractStartDate__c) = 1, MONTH(ContractStartDate__c) = 2), TEXT(YEAR(ADDMONTHS(ContractStartDate__c,1)))&amp;&apos;-Q1&apos;,
IF(OR(MONTH(ContractStartDate__c) = 3, MONTH(ContractStartDate__c) = 4, MONTH(ContractStartDate__c) = 5), TEXT(YEAR(ADDMONTHS(ContractStartDate__c,1)))&amp;&apos;-Q2&apos;,
IF(OR(MONTH(ContractStartDate__c) = 6, MONTH(ContractStartDate__c) = 7, MONTH(ContractStartDate__c) = 8), TEXT(YEAR(ADDMONTHS(ContractStartDate__c,1)))&amp;&apos;-Q3&apos;,
IF(OR(MONTH(ContractStartDate__c) = 9, MONTH(ContractStartDate__c) = 10, MONTH(ContractStartDate__c) = 11), TEXT(YEAR(ADDMONTHS(ContractStartDate__c,1)))&amp;&apos;-Q4&apos;,null)))),null))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contract Start Quarter</label>
