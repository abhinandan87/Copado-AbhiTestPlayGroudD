    <validationRules>
        <fullName>APIAllowedLostReasonCode</fullName>
        <active>true</active>
        <description>SFDC1-6704 : the Reason Code :&apos;Systematically Closed Lost - Past Due&apos; should be allowed via API Users only using integration.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
TEXT(LostReasonCode__c) = &apos;Systematically Closed Lost - Past Due&apos;   &amp;&amp;   
NOT(CONTAINS($Profile.Name, &apos;API&apos;) )
)</errorConditionFormula>
        <errorDisplayField>LostReasonCode__c</errorDisplayField>
        <errorMessage>You can not set this reason.Please select another reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ActivityRequiredForStage5NewBusiness</fullName>
        <active>true</active>
        <description>SFDC1-6066 : Activity is required when opportunity is set on stage &apos;5-Contracting&apos; for New Business.This validation will only be fired when user skip the stage 4 and activity check is skipped.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( 
TEXT(StageName) = &quot;5-Contracting&quot;,
RecordType.Name = &quot;New Business&quot;,
NOT(ActivityCreated__c)
)
)</errorConditionFormula>
        <errorMessage>On Stage 5-Contracting opportunity must have an activity referenced to it.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AutoRenewRestrictionForRestrictedParties</fullName>
        <active>true</active>
        <description>SFDC1-10954 : Prevent users from moving an auto renewal to 6W when there is a restricted party or compliance restriction on the related account</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND
( AutoRenewal__c = True,
DoesThisAutoRenewalHaveChanges__c = False,
AutoRenewStopped__c = False,
$RecordType.Name =&apos;Renewal&apos;,
ISCHANGED(StageName),
ISPICKVAL(StageName, &quot;6W-Renewal Won&quot;),
OR(
OR(UPPER(VCAccountRPSStatus__c)=&apos;RED&apos;,
UPPER(VCAccountRPSStatus__c)= &apos;DOUBLE RED&apos;,
UPPER(VCAccountRPSStatus__c) = &apos;TRIPLE RED&apos;
),
NOT(ISBLANK(ComplianceRestrictions__c))))
)</errorConditionFormula>
        <errorMessage>The customer associated with this opportunity has been flagged with a Red, Double Red, or Triple Red screening or has been flagged with a compliance restriction. Please send an email to Corporate Compliance for guidance on this opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AutoRenewalStoppedContracts</fullName>
        <active>true</active>
        <description>SFDC1-6617;When Auto Renewal = Yes and Does this Auto Renew have Changes? is set to &quot;No&quot; AND Auto Renew Stopped = &quot;Yes&quot; = No -- Do not allow a user to manually close win the opportunity from the opportunity\they must close win by clicking ready for proces</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( ( AutoRenewal__c  = True),
( DoesThisAutoRenewalHaveChanges__c = False),
(AutoRenewStopped__c = True),
($RecordType.Name =&apos;Renewal&apos; ),
ReadyForProcessingContractExist__c == FALSE, 
(ISPICKVAL(StageName, &quot;6W-Renewal Won&quot;)))
)</errorConditionFormula>
        <errorMessage>Please close opportunity by clicking ready for Processing on the contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ChannelPartnerRequiredforCPSalesChannel</fullName>
        <active>true</active>
        <description>SFDC-1633 When &quot;Channel Partner&quot; is selected as a value in the Sales Channel field, the Channel Partner field should then become mandatory</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
ISPICKVAL( SubType__c, &quot;Channel Partner&quot;),
 ISBLANK( ChannelPartner__c ) 
)
&amp;&amp;(NOT ($Permission.ValidationByPassPermission ))
        )</errorConditionFormula>
        <errorMessage>Channel Partner is required for &apos;Channel Partner&apos; Sales Channel</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckCommunityUserasNewOpportunityOwner</fullName>
        <active>true</active>
        <description>SFDC1-4395 IHSMarkit Employees should not be allowed to change the Opportunity Owner to a Channel Partner User.
SFDC1-6387 : Added the SalesOps Profile to allow transfer to Partner User Opp Owner.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( 
    (ISCHANGED(OwnerId)), 
    CONTAINS(Owner.Profile.Name,&quot;IHS Markit: Channel Partner User&quot;),
    NOT(
        OR(
            CONTAINS($Profile.Name,&quot;IHS Markit: Channel Partner User&quot;),
            CONTAINS($Profile.Name,&quot;Sales Ops&quot;),
            CONTAINS($Profile.Name,&quot;User Support&quot;)
        )
    )
)
)</errorConditionFormula>
        <errorMessage>IHSMarkit Employees cannot transfer Opportunity to Channel Partner User</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CompetitorDetailsRequiredWhenLostReason</fullName>
        <active>true</active>
        <description>SFDC-3965 Competitor Details are required when Lost Reason Code is Competition</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
  OR(
    RecordType.Name = &apos;Renewal&apos;,
    RecordType.Name = &apos;New Business&apos;
  ),
  OR(
     CONTAINS(TEXT( StageName), &apos;6W&apos;),
     CONTAINS(TEXT( StageName), &apos;6L&apos;)  
  ),
  ISPICKVAL(LostReasonCode__c, &apos;Competition&apos;),  
  OR(
   ISBLANK(CompetitorName__c),   
   ISBLANK(CompetitorProduct__c),
   ISBLANK(WhyWereWeDisplaced__c),
   ISBLANK(DurationOfSubscription__c)  
  )
)
)</errorConditionFormula>
        <errorMessage>Competitor Name, Competitor product, Why were we displaced and Duration of Subscription fields are required when Lost Reason Code is marked as Competition</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ComplianceStatusIsComplianceApprovalReq</fullName>
        <active>true</active>
        <description>SFDC-5716: Integrate Actimize with Salesforce.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(ISPICKVAL(Account.ComplianceStatus__c, &quot;Compliance Approval Required&quot;))
)</errorConditionFormula>
        <errorMessage>The account for which you have created the opportunity currently has a GFCC Hold in place for the following reason: &quot;Compliance Approval Required&quot;. Please consult with your respective GFCC representative (GFCC_Compliance@spglobal.com).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ComplianceStatusIsDoNotSell</fullName>
        <active>true</active>
        <description>SFDC-5716: Integrate Actimize with Salesforce.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( ISPICKVAL(Account.ComplianceStatus__c, &quot;Do Not Sell&quot;), AND(NOT(ISPICKVAL(StageName, &quot;6L-Closed Lost&quot;)), NOT(ISPICKVAL(StageName, &quot;6L-Renewal Lost&quot;))),
OR(ISCHANGED(StageName),ISNEW())
)
)</errorConditionFormula>
        <errorMessage>The account currently has compliance status: &quot;Do Not Sell&quot;. Please consult with your respective GFCC representative (GFCC_Compliance@spglobal.com).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ComplianceStatusIsGFCCHold</fullName>
        <active>true</active>
        <description>SFDC-5716: Integrate Actimize with Salesforce.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(ISPICKVAL(Account.ComplianceStatus__c, &quot;GFCC Hold&quot;), ISPICKVAL(StageName, &quot;6W-Closed Won&quot;) || ISPICKVAL(StageName, &quot;6W-Renewal Won&quot;) || ISPICKVAL(StageName, &quot;6WP-Closed Won/Order Processed&quot;) || ISPICKVAL(StageName, &quot;6W-Forecasting Placeholder&quot;))
)</errorConditionFormula>
        <errorMessage>The account has a Credit Hold in place for the following reason: &quot;GFCC Hold&quot;. Please consult with your respective GFCC compliance/GFCC representative (GFCC_Compliance@spglobal.com) about this situation to see if the Credit Hold may be removed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractDates</fullName>
        <active>true</active>
        <description>SFDC1-653 Contract Start Date should be less than Contract End Date</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
ContractStartDate__c &gt;  ContractEndDate__c &amp;&amp;(NOT ($Permission.ValidationByPassPermission ))
)</errorConditionFormula>
        <errorMessage>Contract End Date must be Greater than or Equal to Contract Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractEndStartDateReq6WPlaceholder</fullName>
        <active>true</active>
        <description>SFDC-2501 Contract End Date and Contract Start Date must be required when moving to 6W-Forecasting Placeholder stage.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND( ISPICKVAL( StageName, &apos;6W-Forecasting Placeholder&apos;) ,OR( ISBLANK( ContractStartDate__c ) ,  ISBLANK( ContractEndDate__c ) ), RecordType.DeveloperName=&apos;Placeholder&apos;, NOT($Profile.Name = &apos;System Administrator&apos;))
)</errorConditionFormula>
        <errorMessage>Contract Start Date and Contract End Date cannot be blank on Stage: 6W-Forecasting Placeholder.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractStartDateAndEndDateCannotBeNull</fullName>
        <active>true</active>
        <description>SFDC:2015 - Contract Start Date and Contract End Date cannot be null after they have been populated. Sales Ops and System Admin would be an exception to this rule.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.Opportunity__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
    OR(
        AND(
            ISCHANGED(ContractStartDate__c), ISBLANK(ContractStartDate__c)
        ),
        AND(
            ISCHANGED(ContractEndDate__c), ISBLANK(ContractEndDate__c)
        )
    ),
    NOT( 
        OR(  
            $Profile.Name = &quot;System Administrator&quot;,
            $Profile.Name = &quot;IHSMarkit System Admin&quot;, 
            $Profile.Name = &quot;IHSMarkit Sales: Sales Ops&quot;,
            $Profile.Name = &quot;IHSMarkit Sales: User Support&quot;
        )
    )
)
)</errorConditionFormula>
        <errorMessage>After initially populating, contract start date and contract end date cannot be blank.</errorMessage>
    </validationRules>
