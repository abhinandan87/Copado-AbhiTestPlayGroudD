AND (
	AutoRenewal__c == False,
	DoesThisAutoRenewalHaveChanges__c == TRUE
	)
	)</errorConditionFormula>
        <errorMessage>This Opportunity is not an Auto Renewal.</errorMessage>
    </validationRules>
