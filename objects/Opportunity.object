        <fullName>CloseQuarter__c</fullName>
        <description>SFDC-5520: Calculate &apos;Close Date&apos; Quarter Year as per 13month changes for FY2022 and 12month for other Financial Years.</description>
        <externalId>false</externalId>
        <formula>IF( YEAR(CloseDate) &gt; 2021,
IF(OR(MONTH(CloseDate) = 1, MONTH(CloseDate) = 2, MONTH(CloseDate) = 3), TEXT(YEAR(CloseDate))&amp;&apos;-Q1&apos;,
IF(OR(MONTH(CloseDate) = 4, MONTH(CloseDate) = 5, MONTH(CloseDate) = 6), TEXT(YEAR(CloseDate))&amp;&apos;-Q2&apos;,
IF(OR(MONTH(CloseDate) = 7, MONTH(CloseDate) = 8, MONTH(CloseDate) = 9), TEXT(YEAR(CloseDate))&amp;&apos;-Q3&apos;,
IF(OR(MONTH(CloseDate) = 10, MONTH(CloseDate) = 11, MONTH(CloseDate) = 12),TEXT(YEAR(CloseDate))&amp;&apos;-Q4&apos;,null)))),
IF(YEAR(CloseDate) &lt; 2022,IF(OR(MONTH(CloseDate) = 12, MONTH(CloseDate) = 1, MONTH(CloseDate) = 2), TEXT(YEAR(ADDMONTHS(CloseDate,1)))&amp;&apos;-Q1&apos;,
IF(OR(MONTH(CloseDate) = 3, MONTH(CloseDate) = 4, MONTH(CloseDate) = 5), TEXT(YEAR(ADDMONTHS(CloseDate,1)))&amp;&apos;-Q2&apos;,
IF(OR(MONTH(CloseDate) = 6, MONTH(CloseDate) = 7, MONTH(CloseDate) = 8), TEXT(YEAR(ADDMONTHS(CloseDate,1)))&amp;&apos;-Q3&apos;,
IF(OR(MONTH(CloseDate) = 9, MONTH(CloseDate) = 10, MONTH(CloseDate) = 11), TEXT(YEAR(ADDMONTHS(CloseDate,1)))&amp;&apos;-Q4&apos;,null)))),null))</formula>
        <label>Close Quarter</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
