    <validationRules>
        <fullName>ApprovalPendingNottoSubmitted</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND($Permission.WRApprover,
ISPICKVAL(PRIORVALUE(OrderRequestStatus__c), &apos;Approval Pending&apos;),
OR(ISPICKVAL(OrderRequestStatus__c,&apos;Submitted&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Draft&apos;)
)
)
)</errorConditionFormula>
        <errorMessage>Approval Pending can&apos;t be moved to submitted or to Draft.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ErrorForApprovedStatusFromRejected</fullName>
        <active>true</active>
        <description>Error when draft or rejected is moved to Approval Pending</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND($Permission.WRApprover,
OR(ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),ISPICKVAL(OrderRequestStatus__c,&apos;Approval Pending&apos;), ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;) ),
ISPICKVAL(PRIORVALUE(OrderRequestStatus__c), &apos;Rejected&apos;)))</errorConditionFormula>
        <errorMessage>Status can&apos;t be moved to Approved or Approved without Job from Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ErrorForNonApprovers</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND(NOT($Permission.WRApprover) ,
OR(
AND(
ISPICKVAL(PRIORVALUE(OrderRequestStatus__c), &apos;Draft&apos;) ,
OR(ISPICKVAL(OrderRequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approval Pending&apos;)
)
),
AND(
ISPICKVAL(PRIORVALUE(OrderRequestStatus__c), &apos;Submitted&apos;) ,
OR(ISPICKVAL(OrderRequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approval Pending&apos;)
)
),
AND(
ISPICKVAL(PRIORVALUE(OrderRequestStatus__c), &apos;Approval Pending&apos;) ,
OR(ISPICKVAL(OrderRequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Submitted&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Draft&apos;)
)
)
)
)
)</errorConditionFormula>
        <errorMessage>You are not authorized to make this change as you are not an approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NewRecordStatusDraftOrSubmitted</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,ISBLANK(Id) &amp;&amp; OR (ISPICKVAL( OrderRequestStatus__c ,&apos;Rejected&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approval Pending&apos;)))</errorConditionFormula>
        <errorMessage>You are not authorized to change to this Request Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OrderRequeststatustoSubmitfromDraft</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,ISPICKVAL(PRIORVALUE(OrderRequestStatus__c), &apos;Draft&apos;) &amp;&amp; OR(ISPICKVAL(OrderRequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approval Pending&apos;)
) &amp;&amp; $Permission.WRApprover)</errorConditionFormula>
        <errorMessage>Please move the Order Request Status to Submitted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RejectedLogtoSubmitOrDraft</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND(ISPICKVAL(PRIORVALUE(OrderRequestStatus__c),&apos;Rejected&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(OrderRequestStatus__c,&apos;Approval Pending&apos;)))</errorConditionFormula>
        <errorMessage>Please update the status to Submitted or Draft from Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubmittedRecordtoDraft</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.OrderRequest__c, $Setup.SystemMutedValidationRules__c.All__c ), False,ISPICKVAL( OrderRequestStatus__c ,&apos;Draft&apos;) &amp;&amp;
ISPICKVAL( PRIORVALUE(OrderRequestStatus__c) ,&apos;Submitted&apos;))</errorConditionFormula>
        <errorMessage>Order Request can&apos;t be moved back to Draft.</errorMessage>
    </validationRules>
