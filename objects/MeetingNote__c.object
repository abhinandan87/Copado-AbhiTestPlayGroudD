        <externalId>false</externalId>
        <formula>Street__c +
IF(AND(NOT(ISBLANK(Street__c)), NOT(ISBLANK(City__c)) || NOT(ISBLANK(StateProvince__c)) || NOT(ISBLANK(ZipPostalCode__c))), BR(), &apos;&apos;) + 
IF(NOT(ISBLANK(City__c)), City__c + &apos;, &apos; + StateProvince__c + &apos; &apos; + ZipPostalCode__c, StateProvince__c + &apos; &apos; + ZipPostalCode__c) +
IF(NOT(ISBLANK(Street__c)) || NOT(ISBLANK(City__c)) || NOT(ISBLANK(StateProvince__c)) || NOT(ISBLANK(ZipPostalCode__c)), BR(), &apos;&apos;) + 
IF(NOT(ISBLANK(Country__c)), Country__c, &apos;&apos;)</formula>
