    <validationRules>
        <fullName>PriceIncreaseAppliedValidation</fullName>
        <active>true</active>
        <description>SFDC1-9299:If &apos;Price Increase Applied&apos; = &apos;No&apos;, then &apos;Comments&apos; field is required</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.PriceIncreaseDetails__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(
NOT(ISBLANK(TEXT(PriceIncreaseApplied__c))),
ISPICKVAL(PriceIncreaseApplied__c , &apos;No&apos;),
ISBLANK(Comments__c)
)
)</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>If &apos;Price Increase Applied&apos; = &apos;No&apos;, then &apos;Comments&apos; field is required.</errorMessage>
    </validationRules>
