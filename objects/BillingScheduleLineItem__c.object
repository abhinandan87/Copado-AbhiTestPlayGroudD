    <validationRules>
        <fullName>BilledContractEditRestriction</fullName>
        <active>true</active>
        <description>SFDC1-4607 Only System Administrators can edit a Billing Schedule Line Item record if Billed is marked as true. SFDC1-9974 : updated the Error Message and added Profiles</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
NOT(ISNEW()),
Billed__c == TRUE,
NOT(ISCHANGED(Billed__c)),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;, 
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin: API&quot;,
NOT(CONTAINS($Profile.Name, &quot;Order Management&quot;))
))</errorConditionFormula>
        <errorMessage>You can not edit Volume/Variable Billing Line Item once it is &apos;Billed&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BilledEditRestriction</fullName>
        <active>true</active>
        <description>SFDC1-10150 : &apos;Billed&apos; should only be edited by System Admin, Order Management Users.</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND( 
		ContractLineItem__r.Contract__r.RecordType.DeveloperName = &quot;Consulting&quot;,
ISCHANGED(Billed__c), 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;, 
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin: API&quot;, 
NOT(CONTAINS($Profile.Name, &quot;Order Management&quot;)) 
))</errorConditionFormula>
        <errorDisplayField>Billed__c</errorDisplayField>
        <errorMessage>You are not authorized to update &apos;Billed&apos; on Volume/Variable Billing Line Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillingStartDateCannotBeBlank</fullName>
        <active>true</active>
        <description>SFDC1-4607 Billing Start Date cannot be blank when &apos;Ready for Billing&apos; is marked true</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
 ReadyForBilling__c == TRUE,
 ISBLANK(BillingStartDate__c) 
))</errorConditionFormula>
        <errorDisplayField>BillingStartDate__c</errorDisplayField>
        <errorMessage>Invoicing Month cannot be blank when &apos;Ready for Billing&apos; is marked true</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CLIBillingPeriodsNotBlankAtBSLICreate</fullName>
        <active>true</active>
        <description>SFDC1-7472 : This validation will prevent &apos;Billing Schedule Line Item&apos; record creation from CLI if Billing Periods is Blank on CLI.</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
ISNEW() ,
NOT( ISBLANK(ContractLineItem__c) ) ,  ISBLANK(ContractLineItem__r.BillingPeriods__c)
))</errorConditionFormula>
        <errorMessage>Please enter Contract Line Item&apos;s Billing Periods before creating Volume/Variable Billing Line Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CLIBillingScheduleNotBlankAtBSLICreate</fullName>
        <active>true</active>
        <description>SFDC1-9908 : This validation will prevent &apos;Billing Schedule Line Item&apos; record creation from CLI if Billing Schedule Type is Blank on CLI.</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
ISNEW() ,
NOT( ISBLANK(ContractLineItem__c) ) , TEXT(ContractLineItem__r.BillingScheduleType__c) = &apos;&apos;
))</errorConditionFormula>
        <errorMessage>Please enter Contract Line Item&apos;s Billing Schedule Type before creating Volume/Variable Billing Line Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ContractCurrencyCheck</fullName>
        <active>true</active>
        <description>SFDC - 1438 - Volume/Variable Billing Line Item Currency</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND (TEXT(CurrencyIsoCode) &lt;&gt; ContractLineItem__r.Contract__r.Currency__c))</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>Incorrect Currency Code, Please Refer to the Contract and Update Accordingly</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LockBillingScheduleForSalesProfile</fullName>
        <active>true</active>
        <description>SFDC1-9611:Polk: Sales Reps and Sales Coordinators need the ability to Edit Billing Schedule Line Items.
SFDC1-9973:Polk: Sales Reps and Sales Coordinators need the ability to Edit Billing Schedule Line Items on Contract Order Forms</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND( 
NOT(ISNEW()), 
CONTAINS($Profile.Name,&quot;Sales&quot;), 
NOT(OR(ContractLineItem__r.Contract__r.RecordType.DeveloperName = &quot;Consulting&quot;,ContractLineItem__r.Contract__r.RecordType.DeveloperName = &quot;OrderForm&quot;) 
) 
))</errorConditionFormula>
        <errorMessage>You are not authorized to update Volume/Variable Billing Line Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QuantityCannotBeBlank</fullName>
        <active>true</active>
        <description>SFDC1-4607 Quantity cannot be blank when &apos;Ready for Billing&apos; is marked true</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
 ReadyForBilling__c == TRUE,
 ISBLANK(Quantity__c) 
))</errorConditionFormula>
        <errorDisplayField>Quantity__c</errorDisplayField>
        <errorMessage>Quantity cannot be blank when &apos;Ready for Billing&apos; is marked true</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReadyForBillingEditRestriction</fullName>
        <active>true</active>
        <description>SFDC1-10150 : &apos;Ready for Billing&apos; should only be edited by System Admin, Order Management and Revenue Assurance Permission Set Users.</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND( 
ContractLineItem__r.Contract__r.RecordType.DeveloperName = &quot;Consulting&quot;,
ISCHANGED(ReadyForBilling__c), 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin&quot;, 
$Profile.Name &lt;&gt; &quot;IHSMarkit System Admin: API&quot;, 
NOT(CONTAINS($Profile.Name, &quot;Order Management&quot;)), 
NOT($Permission.RevenueAssuranceForBSLI) 
))</errorConditionFormula>
        <errorDisplayField>ReadyForBilling__c</errorDisplayField>
        <errorMessage>You are not authorized to update &apos;Ready for Billing&apos; on Volume/Variable Billing Line Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReadyForBillingRestriction</fullName>
        <active>true</active>
        <description>SFDC-2004: Volume/Variable Billing Line Items should not be allowed to be marked ‘Ready for Billing’ unless the contract is at stage 10 or 11.</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
NOT(ISPICKVAL(Contract__r.Status,&quot;ReadyForProcessing&quot;)),
NOT(ISPICKVAL(Contract__r.Status,&quot;Complete&quot;)),
ReadyForBilling__c = TRUE))</errorConditionFormula>
        <errorMessage>Contract Stage should be set to Stage 10-Ready for Processing as &apos;Ready for billing’ field should only be checked when stage is 10</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UnitPriceCannotBeBlank</fullName>
        <active>true</active>
        <description>SFDC1-4607 Unit Price cannot be blank when &apos;Ready for Billing&apos; is marked true</description>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.All__c, $Setup.SystemMutedValidationRules__c.BillingScheduleLineItem__c ), False,AND(
 ReadyForBilling__c == TRUE,
 ISBLANK(UnitPrice__c) 
))</errorConditionFormula>
        <errorDisplayField>UnitPrice__c</errorDisplayField>
        <errorMessage>Unit Price cannot be blank when &apos;Ready for Billing&apos; is marked true</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>