    <fields>
        <fullName>AmIAssignedRep__c</fullName>
        <description>This field is used for WRF-POLK.</description>
        <externalId>false</externalId>
        <formula>IF(  WorkRequestForm__r.PrimarySalesContact__c  = $User.Id ,&quot;True&quot;,&quot;False&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Am I Assigned WR Rep?</label>
        <required>false</required>
