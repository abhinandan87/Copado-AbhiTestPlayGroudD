    <fields>
        <fullName>ActiveDaysOnHold__c</fullName>
        <description>This field is used for WRF-POLK.</description>
        <externalId>false</externalId>
        <formula>IF (  (NOT (ISNULL( DateOnHold1__c))&amp;&amp; 
      ISNULL( DateReleasedFromHold1__c) )
	, TODAY() - DateOnHold1__c
	, IF (  ( NOT (ISNULL( DateOnHold2__c))&amp;&amp; 
		      ISNULL( DateReleasedFromHold2__c) )
		, TODAY() - DateOnHold2__c
			, IF (  ( NOT (ISNULL( DateOnHold3__c))&amp;&amp; 
			      ISNULL( DateReleasedFromHold3__c) )
			, TODAY() - DateOnHold3__c
				, IF (  ( NOT (ISNULL( DateOnHold4__c))&amp;&amp; 
				      ISNULL( DateReleasedFromHold4__c) )
				, TODAY() - DateOnHold4__c
					, IF (  ( NOT (ISNULL( DateOnHold5__c))&amp;&amp; 
					      ISNULL( DateReleasedFromHold5__c) )
					, TODAY() - DateOnHold5__c
					, 0))))
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active Days On Hold</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
