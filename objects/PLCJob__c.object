    <validationRules>
        <fullName>DateTimeOfDeliveryExceededNegotiatedDate</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISBLANK(DateTimeOfDelivery__c)) &amp;&amp; (DATEVALUE(DateTimeOfDelivery__c) &gt; NegotiatedDate__c) &amp;&amp;
(LateDeliverables__c == 0 || ISBLANK(LateDeliverables__c))</errorConditionFormula>
        <errorMessage>Date &amp; Time of Delivery is greater than the Negotiated Date. Please enter value &gt; 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EncryptionKeyIsBlank</fullName>
        <active>true</active>
        <errorConditionFormula>EncryptionRequired__c = true &amp;&amp; ISBLANK(EncryptionKey__c)</errorConditionFormula>
        <errorDisplayField>EncryptionKey__c</errorDisplayField>
        <errorMessage>Field Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EncryptionRequiredIsTrue</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.PLCJob__c, $Setup.SystemMutedValidationRules__c.All__c), False,EncryptionRequired__c = false &amp;&amp; NOT(ISBLANK(EncryptionKey__c)))</errorConditionFormula>
        <errorDisplayField>EncryptionKey__c</errorDisplayField>
        <errorMessage>&apos;Encryption Key&apos; cannot be filled unless &apos;Encryption Required &apos; is true</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FTPRequired</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.PLCJob__c, $Setup.SystemMutedValidationRules__c.All__c), False,FTPRequired__c &lt;&gt; true &amp;&amp; (NOT(ISBLANK(FTPPath__c))|| NOT(ISBLANK(FTPUserID__c)) || FTPInformationInWRF__c = true))</errorConditionFormula>
        <errorMessage>&apos;FTP Information in WRF,&apos;FTP Path&apos;, &apos;FTP User ID&apos; cannot be filled unless &apos;FTP Required &apos; is True</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>JobCompletedDeliveredConditionstobeTrue</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(JobCompletedDelivered__c) &amp;&amp;
JobCompletedDelivered__c = True &amp;&amp; 
(
  ISBLANK(TotalDeliverables__c) 
  || ISBLANK(DateTimeOfDelivery__c)
  || ISBLANK(DefectiveDeliverables__c)
  || ISBLANK(LateDeliverables__c)
  || OpenTasks__c &gt; 1
  || 
  (
   ISBLANK(RoyaltyRecordsIncluded__c)
   &amp;&amp; 
   if(INCLUDES(WorkRequestForm__r.RoyaltyVendor__c,&apos;No Royalty Data&apos;),false,true) 
   &amp;&amp; 
   NOT(ISBLANK(WorkRequestForm__r.RoyaltyVendor__c))
  )
)</errorConditionFormula>
        <errorDisplayField>JobCompletedDelivered__c</errorDisplayField>
        <errorMessage>You cannot complete a job unless no more than one task remains open and the following fields have been entered: Date &amp; Time Of Delivery, Total Deliverables, Defective Deliverables, Late Deliverables, and Royalty Records Included.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PSCCodeReviewIsBlank</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(JobType__c, &apos;Development&apos;)  &amp;&amp; TotalJobEstimate__c &gt;= 25 &amp;&amp; (PSCCodeReview__c == &apos;&apos; || PSCCodeReview__c == NULL)</errorConditionFormula>
        <errorDisplayField>PSCCodeReview__c</errorDisplayField>
        <errorMessage>Field Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PeerReviewCorrected</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR($Setup.SystemMutedValidationRules__c.PLCJob__c, $Setup.SystemMutedValidationRules__c.All__c), False,NOT(ISPICKVAL(PeerReviewStatus__c, &apos;Corrected&apos;)) &amp;&amp; (NOT(ISPICKVAL(ReasonForFailure__c, &apos;&apos;)) || NOT(ISBLANK(TotalReworkHours__c))))</errorConditionFormula>
        <errorMessage>&apos;Reason For Failure&apos;, &apos;Total Rework Hours&apos; cannot be filled unless &apos;Peer Review Status&apos; is Corrected</errorMessage>
