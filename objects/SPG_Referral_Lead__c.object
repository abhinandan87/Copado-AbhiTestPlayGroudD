        <defaultValue>&quot;Created&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Tracks status of informatica routing the referral to each individual division. If Informatica does not route the referral successfully, the status will remain &quot;Created&quot; until the issue is fixed and the referrals is picked up in the next batch.</description>
        <externalId>false</externalId>
        <inlineHelpText>If status is &quot;routed&quot;, this lead is successfully created in the receiving division. If the status is &quot;Created&quot; or &quot;Transferred&quot;, then the lead has not be received by the division.</inlineHelpText>
        <label>Routing Status</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Created</fullName>
                    <default>false</default>
                    <label>Created</label>
                </value>
                <value>
                    <fullName>Routed</fullName>
                    <default>false</default>
                    <label>Routed</label>
                </value>
                <value>
                    <fullName>Transferred</fullName>
                    <default>false</default>
                    <label>Transferred</label>
                </value>
            </valueSetDefinition>
        </valueSet>
