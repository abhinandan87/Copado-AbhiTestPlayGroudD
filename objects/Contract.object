    <fields>
        <fullName>ContractStartQuarter__c</fullName>
        <description>SFDC-5520: Calculate &apos;Contract Start Date&apos; Quarter Year as per 13month changes for FY2022 and 12month for other Financial Years.</description>
        <externalId>false</externalId>
        <formula>IF( YEAR(StartDate) &gt; 2021,
IF(OR(MONTH(StartDate) = 1, MONTH(StartDate) = 2, MONTH(StartDate) = 3), TEXT(YEAR(StartDate))&amp;&apos;-Q1&apos;,
IF(OR(MONTH(StartDate) = 4, MONTH(StartDate) = 5, MONTH(StartDate) = 6), TEXT(YEAR(StartDate))&amp;&apos;-Q2&apos;,
IF(OR(MONTH(StartDate) = 7, MONTH(StartDate) = 8, MONTH(StartDate) = 9), TEXT(YEAR(StartDate))&amp;&apos;-Q3&apos;,
IF(OR(MONTH(StartDate) = 10, MONTH(StartDate) = 11, MONTH(StartDate) = 12), TEXT(YEAR(StartDate))&amp;&apos;-Q4&apos;,null)))),
IF( YEAR(StartDate) &lt; 2022,IF(OR(MONTH(StartDate) = 12, MONTH(StartDate) = 1, MONTH(StartDate) = 2), TEXT(YEAR(ADDMONTHS(StartDate,1)))&amp;&apos;-Q1&apos;,
IF(OR(MONTH(StartDate) = 3, MONTH(StartDate) = 4, MONTH(StartDate) = 5), TEXT(YEAR(ADDMONTHS(StartDate,1)))&amp;&apos;-Q2&apos;,
IF(OR(MONTH(StartDate) = 6, MONTH(StartDate) = 7, MONTH(StartDate) = 8), TEXT(YEAR(ADDMONTHS(StartDate,1)))&amp;&apos;-Q3&apos;,
