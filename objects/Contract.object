	RecordType.Name == &apos;Consulting&apos;, 
	NOT(ISNEW()),
	Fastrack__c == FALSE, 
        Opportunity__r.RecordType.Name&lt;&gt;&apos;Gratis&apos;,
        OR(
                TEXT(Status) == &apos;Generation&apos;,
                TEXT(Status) == &apos;ReadyforProcessing&apos;),
	ByPassValidationsForProcessBuilder__c == FALSE, 
	NOT(ISCHANGED(ByPassValidationsForProcessBuilder__c)),
	NOT(ISCHANGED(TotalContractValue__c)),
	NOT(ISCHANGED(FirstYearContractValue__c)),
	AND (ISPICKVAL(GeneralAgreementType__c,&apos;Other&apos;),
		ISBLANK(GeneralAgreementDate__c))	 
))</errorConditionFormula>
        <errorDisplayField>GeneralAgreementDate__c</errorDisplayField>
        <errorMessage>Agreement Effective Date is mandatory, please update the Contract accordingly to proceed further.</errorMessage>
    </validationRules>
