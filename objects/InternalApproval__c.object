    <fields>
        <fullName>Contact__c</fullName>
        <description>SFDC1-6282: Contact
SFDC1-9701: Only emailing to internal IHSMarkit Users when using the internal approvals
SFDC1-9979: Filter Inactive user with IHMarkit mail ID
SFDC1-10610: update Internal Approval&apos;s Contact Lookup filter message
SFDC-3911: Skip filter for batch admin</description>
        <externalId>false</externalId>
        <inlineHelpText>The person contact who this Internal Approval request will be sent to.</inlineHelpText>
        <label>Contact</label>
        <lookupFilter>
            <active>true</active>
            <booleanFilter>(1 AND 2) OR 3</booleanFilter>
            <errorMessage>Please select an internal IHS Markit user contact (email address ends with &quot;@ihsmarkit.com&quot;).</errorMessage>
            <filterItems>
                <field>Contact.Email</field>
                <operation>contains</operation>
                <value>ihsmarkit.com</value>
            </filterItems>
            <filterItems>
                <field>Contact.Status__c</field>
                <operation>equals</operation>
                <value>Active</value>
            </filterItems>
            <filterItems>
                <field>$User.FullName__c</field>
                <operation>equals</operation>
                <value>Batch Admin</value>
            </filterItems>
            <infoMessage>Only internal contact is allowed for Internal Approvals.</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Internal Approvals</relationshipLabel>
        <relationshipName>Internal_Approvals</relationshipName>
        <relationshipOrder>0</relationshipOrder>
