    <validationRules>
        <fullName>ApprovalPendingNottoSubmitted</fullName>
        <active>true</active>
        <description>Approval Pending can&apos;t be moved to submitted or to Draft</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND($Permission.WRApprover,
ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Approval Pending&apos;),
OR(ISPICKVAL(RequestStatus__c,&apos;Submitted&apos;),
ISPICKVAL(RequestStatus__c,&apos;Draft&apos;)
)
)
)</errorConditionFormula>
        <errorMessage>Status can&apos;t be moved to Draft or Submitted from Approval Pending status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ErrorForApprovalPending</fullName>
        <active>true</active>
        <description>Error when draft or rejected is moved to Approval Pending</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND($Permission.WRApprover,
AND(
OR(ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Draft&apos;) ,ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Rejected&apos;)), 
ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;)
)
)
)</errorConditionFormula>
        <errorMessage>You can&apos;t move the status to Approval pending from Draft or Rejected status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ErrorForApprovedStatusFromRejected</fullName>
        <active>true</active>
        <description>Shouldn&apos;t move status directly from rejected to approved.</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND($Permission.WRApprover,
OR(ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;), ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;)  ),
ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Rejected&apos;)))</errorConditionFormula>
        <errorMessage>Status can&apos;t be moved to Approved or Approved without Job from Rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ErrorForNonApprovers</fullName>
        <active>true</active>
        <description>Users without Approve permission must not be able to change the request status other than draft or submitted</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,
AND(NOT($Permission.WRApprover) ,
OR(
AND(
ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Draft&apos;) , 
OR(ISPICKVAL(RequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;)
)
),
AND(
ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Submitted&apos;) , 
OR(ISPICKVAL(RequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;)
)
),
AND(
ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Approval Pending&apos;) , 
OR(ISPICKVAL(RequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(RequestStatus__c,&apos;Submitted&apos;),
ISPICKVAL(RequestStatus__c,&apos;Draft&apos;)
)
),
ISCHANGED( ProductionComments__c)
)
)
)</errorConditionFormula>
        <errorMessage>You are not authorized to make this change as you are not an approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NewRecordStatusDraftOrSubmitted</fullName>
        <active>true</active>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,ISBLANK(Id) &amp;&amp; OR (ISPICKVAL(RequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;)))</errorConditionFormula>
        <errorMessage>You are not authorized to change to this Request Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductionCommentsRequiredToReject</fullName>
        <active>true</active>
        <description>Production Comments is required to reject the change log</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), 
False,
AND(ISPICKVAL(RequestStatus__c ,&quot;Rejected&quot;), OR(ISPICKVAL(PRIORVALUE(RequestStatus__c) , &quot;Submitted&quot;),ISPICKVAL(PRIORVALUE(RequestStatus__c) ,&quot;Approval Pending&quot;),ISPICKVAL(PRIORVALUE(RequestStatus__c) ,&quot;Rejected&quot;))  ,
OR(ISBLANK(ProductionComments__c), ISNULL(ProductionComments__c) ) ,  $Permission.WRApprover ,!ISBLANK(Id)))</errorConditionFormula>
        <errorMessage>Production comments are required when rejecting a Change Log.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RejectedLogtoSubmitOrDraft</fullName>
        <active>true</active>
        <description>Rejected Log Must be moved to Submitted or Draft</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,AND(ISPICKVAL(PRIORVALUE(RequestStatus__c),&quot;Rejected&quot;),
ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;)))</errorConditionFormula>
        <errorMessage>Please update the status to Submitted or Draft from Rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequeststatustoSubmitfromDraft</fullName>
        <active>true</active>
        <description>Request status must be moved to Submit from Draft</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), False,ISPICKVAL(PRIORVALUE(RequestStatus__c), &apos;Draft&apos;) &amp;&amp;   OR(ISPICKVAL(RequestStatus__c,&apos;Rejected&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approved without Job&apos;),
ISPICKVAL(RequestStatus__c,&apos;Approval Pending&apos;)
) &amp;&amp; $Permission.WRApprover)</errorConditionFormula>
        <errorMessage>Please move the Request Status to Submitted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SubmittedRecordtoDraft</fullName>
        <active>true</active>
        <description>Submitted or Approval Pending change log must not be moved directly to draft</description>
        <errorConditionFormula>If(OR( $Setup.SystemMutedValidationRules__c.ChangeLog__c, $Setup.SystemMutedValidationRules__c.All__c ), 
False,
ISPICKVAL( RequestStatus__c ,&apos;Draft&apos;) &amp;&amp;  
ISPICKVAL( PRIORVALUE(RequestStatus__c) ,&apos;Submitted&apos;))</errorConditionFormula>
        <errorMessage>Change Log can&apos;t be moved back to Draft.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>