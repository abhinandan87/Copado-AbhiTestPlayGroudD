    <validationRules>
        <fullName>Account_and_Oppy_blank</fullName>
        <active>true</active>
        <description>US 5306779 : Account or Opportunity should be blank when Not applicable is checked</description>
        <errorConditionFormula>!(ISBLANK( Account__c ) &amp;&amp; ISBLANK( Opportunity__c )) &amp;&amp;  Not_applicable__c</errorConditionFormula>
        <errorDisplayField>Not_applicable__c</errorDisplayField>
        <errorMessage>Please uncheck the &apos;Not Applicable&apos; checkbox, if Account or Opportunity is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_and_Oppy_required</fullName>
        <active>true</active>
        <description>US 5306779 : Account or Opportunity is required if the not applicable checkbox is unchecked</description>
        <errorConditionFormula>ISBLANK( Account__c ) &amp;&amp;  ISBLANK( Opportunity__c ) &amp;&amp;  !Not_applicable__c</errorConditionFormula>
        <errorMessage>To proceed, please check the Not Applicable checkbox if Account or Opportunity is not selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Updates</fullName>
        <active>true</active>
        <description>US 5306779: Restrict updates on the request if the status is not new</description>
        <errorConditionFormula>!ISPICKVAL( Status__c , &apos;New&apos;) &amp;&amp; (ISCHANGED( Account__c ) || ISCHANGED( Opportunity__c ) || ISCHANGED(  Description__c ) || ISCHANGED( Industry__c ) || ISCHANGED( Category__c ) || ISCHANGED( Division__c ) || ISCHANGED(  Not_applicable__c  ))</errorConditionFormula>
        <errorMessage>The request can only be updated when status is &apos;New&apos;</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
